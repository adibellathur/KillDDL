<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_calendar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="0px"
    tools:context="com.team27.killddl.CalendarFragment"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <!-- TODO: Update blank fragment layout -->

    <android.support.design.widget.TabLayout
        android:id="@+id/calendar_tab_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:tabGravity="fill"
        app:tabMode="fixed"
        app:tabSelectedTextColor="?android:attr/textColorPrimary"
        app:tabTextColor="?android:attr/textColorPrimaryInverse"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="1dp" />

    <android.support.v4.view.ViewPager
        android:id="@+id/calendar_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</FrameLayout>
